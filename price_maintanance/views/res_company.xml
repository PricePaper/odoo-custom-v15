<odoo>
    <data>
        <record id="res_company_inherit_sale_product_lst_price_update" model="ir.ui.view">
            <field name="name">res.company.pricepaper.sale.product.lst.price</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="price_paper.res_company_inherit_price_paper"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='sale_history_months']" position="after">
                    <field name='product_lst_price_months'/>
                    <field name='partner_count'/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
