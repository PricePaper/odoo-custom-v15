<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="res_partner_inherit_stock_orderpoint" model="ir.ui.view">
            <field name="name">res.partner.stock.orderpoint.pricepaper</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="account.view_partner_property_form"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook/page[@name='sales_purchases']/group/group[2]/field[@name='property_supplier_payment_term_id']"
                       position="before">
                    <field name="supplier_rank" invisible="1"/>
                    <field name="delay" attrs="{'invisible': [('supplier_rank', '=', 0)]}"/>
                    <field name="order_freq" attrs="{'invisible': [('supplier_rank', '=', 0)]}"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
