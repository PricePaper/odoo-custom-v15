<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_account_journal_form_inherited_autherize_extension" model="ir.ui.view">
            <field name="name">account.journal.form.inherited.autherize.extension</field>
            <field name="model">account.journal</field>
            <field name="inherit_id" ref="account.view_account_journal_form" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='type']" position="after">
                  <field name="is_autherize_net" widget="boolean_toggle"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
